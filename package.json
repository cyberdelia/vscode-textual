{
  "name": "textual",
  "version": "0.0.1",
  "publisher": "cyberdelia",
  "repository": "https://github.com/cyberdelia/textual-vscode",
  "engines": {
    "vscode": "^1.32.0"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "title": "Enable Textual",
        "command": "textual.enableTextual",
        "category": "Textual"
      },
      {
        "title": "Disable Textual",
        "command": "textual.disableTextual",
        "category": "Textual"
      }
    ],
    "configuration": {
      "properties": {
        "textual.enableTextual": {
          "type": "boolean",
          "default": true
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint --fix . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.34.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.0.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "text-readability": "^1.0.5"
  }
}
